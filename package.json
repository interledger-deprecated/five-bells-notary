{
  "name": "five-bells-notary",
  "version": "4.0.4",
  "description": "Consensus-capable Byzantine fault tolerant cryptographic notary",
  "repository": {
    "type": "git",
    "url": "git@github.com:ripple/five-bells-notary.git"
  },
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start-prof": "node --prof --logfile=${NOTARY_V8_LOGFILE:-v8.log} app.js",
    "lint": "eslint src test *.js",
    "apidoc": "apidoc -o apidoc-out -i src/controllers/",
    "test": "istanbul test -- _mocha",
    "test-oracle": "NOTARY_UNIT_DB_URI='oracle://system:oracle@192.168.99.100:1521/' DYLD_LIBRARY_PATH=/opt/oracle/instantclient LD_LIBRARY_PATH=/opt/oracle/instantclient node node_modules/.bin/istanbul test -- _mocha",
    "bump": "version-bump",
    "integration": "integration all",
    "report-coverage": "codecov",
    "ci-npm-publish": "ci-publish"
  },
  "keywords": [
    "consensus",
    "witness",
    "atomic",
    "transaction",
    "payment"
  ],
  "author": "Stefan Thomas <justmoon@members.fsf.org>",
  "license": "ISC",
  "dependencies": {
    "co": "^4.6.0",
    "co-body": "^4.0.0",
    "co-defer": "^1.0.0",
    "co-emitter": "^0.2.3",
    "co-map": "^1.1.0",
    "co-request": "^1.0.0",
    "constitute": "^1.2.0",
    "five-bells-condition": "^3.1.0",
    "five-bells-shared": "^17.0.0",
    "knex": "^0.9.0",
    "koa": "^1.0.0",
    "koa-mag": "^1.1.0",
    "koa-router": "^5.1.2",
    "lodash": "^3.10.1",
    "mag": "^0.9.1",
    "mag-hub": "^0.1.1",
    "moment": "^2.10.6",
    "mysql": "^2.9.0",
    "priorityqueuejs": "^1.0.0"
  },
  "optionalDependencies": {
    "mysql": "^2.9.0",
    "pg": "^4.4.1",
    "sqlite3": "^3.1.0",
    "tedious": "^1.12.3"
  },
  "devDependencies": {
    "apidoc": "^0.13.1",
    "chai": "^3.4.1",
    "co-mocha": "^1.1.2",
    "co-supertest": "0.0.10",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.9",
    "eslint": "^2.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "five-bells-integration-test": "2.1.0",
    "istanbul": "^0.4.0",
    "methods": "^1.1.1",
    "mocha": "^2.3.4",
    "nock": "^3.1.1",
    "sinon": "^1.17.2",
    "spec-xunit-file": "0.0.1-3",
    "supertest": "^1.1.0"
  }
}
